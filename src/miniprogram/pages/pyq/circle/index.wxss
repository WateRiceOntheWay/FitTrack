.li-imgs {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin-top: 8rpx;
}

.li-img-one {
    padding: 4rpx;
    width: 200rpx;
    height: 200rpx;
}


.head-loading{
    animation: loading_rotate 2s linear infinite;
}

@keyframes loading_rotate {
    0% {transform : rotate(0deg);}
    100% {transform : rotate(360deg);}
}